        android:theme="@style/Theme.KERJAIN">

        <activity
            android:name=".WelcomeActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TipePekerjaanActivity"
            android:exported="false" />
        <activity android:name=".LoginPelamarActivity"
            android:exported="false" />
        <activity android:name=".LoginperusahaanActivity"
            android:exported="false" />
        <activity android:name=".Home_PelamarActivity"
            android:exported="false" />
        <activity android:name=".Home_CompanyActivity"
            android:exported="false" />


    </application>
</manifest>
